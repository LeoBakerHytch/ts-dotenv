{
    "name": "ts-dotenv",
    "version": "0.8.3",
    "description": "Strongly-typed environment variables from .env & process.env",
    "author": "Leo Baker-Hytch",
    "license": "MIT",
    "keywords": [
        "env",
        "dotenv",
        "environment",
        "variables",
        "config",
        "typescript"
    ],
    "homepage": "https://github.com/LeoBakerHytch/ts-dotenv#readme",
    "bugs": {
        "url": "https://github.com/LeoBakerHytch/ts-dotenv/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LeoBakerHytch/ts-dotenv.git"
    },
    "main": "index.js",
    "types": "index.d.ts",
    "files": [
        "index.js",
        "index.js.map",
        "index.d.ts",
        "lib/*.js",
        "lib/*.js.map",
        "lib/*.d.ts"
    ],
    "scripts": {
        "build": "tsc --build tsconfig.dist.json",
        "coverage": "jest --coverage",
        "clean": "rm -rf index.{js,js.map,d.ts} ./lib/*.{js,js.map,d.ts} ./tests/*.{js,js.map,d.ts}",
        "prepublishOnly": "npm run build",
        "test": "jest"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^29.2.2",
        "@types/node": "^18.11.9",
        "coveralls": "^3.1.1",
        "jest": "^29.3.0",
        "prettier": "^2.7.1",
        "ts-jest": "^29.0.3",
        "typescript": "^4.8.4"
    }
}
