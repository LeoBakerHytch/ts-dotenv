{
    "name": "ts-dotenv",
    "version": "0.8.3",
    "description": "Strongly-typed environment variables from .env & process.env",
    "author": "Leo Baker-Hytch",
    "license": "MIT",
    "keywords": [
        "env",
        "dotenv",
        "environment",
        "variables",
        "config",
        "typescript"
    ],
    "homepage": "https://github.com/LeoBakerHytch/ts-dotenv#readme",
    "bugs": {
        "url": "https://github.com/LeoBakerHytch/ts-dotenv/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LeoBakerHytch/ts-dotenv.git"
    },
    "main": "index.js",
    "types": "index.d.ts",
    "files": [
        "**/*.js",
        "**/*.js.map",
        "**/*.d.ts",
        "!tests",
        "!coverage",
        "!jest.config.js"
    ],
    "scripts": {
        "build": "tsc --build tsconfig.dist.json",
        "clean": "rm -rf index.{js,js.map,d.ts} ./lib/*.{js,js.map,d.ts} ./tests/*.{js,js.map,d.ts}",
        "test": "jest",
        "prepublishOnly": "npm run build"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^26.0.15",
        "@types/node": "^14.14.10",
        "coveralls": "^3.0.6",
        "jest": "^25.1.0",
        "prettier": "^2.0.2",
        "ts-jest": "^25.2.1",
        "typescript": "^3.6.2"
    }
}
